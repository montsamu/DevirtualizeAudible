{
  "manifest_version": 3,
  "name": "DevirtualizeAudible",
  "version": "1.0",

  "description": "Removes 'Virtual Voice' narrations from product listings at Audible.",
  "action" : {
    "default_icon": "icons/border-48.png",
    "default_popup": "devirtualizeAudible.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*.audible.com/*"],
      "js": ["devirtualizeAudible.js"]
    }
  ]
}

